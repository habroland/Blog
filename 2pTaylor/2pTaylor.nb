Cell[CellGroupData[{

Cell["Two - point Taylor expansion", "Chapter",
 CellChangeTimes->{{3.8010672791285896`*^9, 
  3.8010672905801997`*^9}},ExpressionUUID->"aeb8404a-91f8-456f-99bc-\
d8b695f09cdc"],

Cell["\<\
I use a recursive method in the leading order to define the functions as \
shown in the reference\
\>", "Text",
 CellChangeTimes->{{3.801067410751011*^9, 
  3.8010674539809985`*^9}},ExpressionUUID->"0540af61-6dba-4f98-b93e-\
acaa2cf99281"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"TwoPointTaylor", "[", 
   RowBox[{"f_", ",", "a_", ",", "b_", ",", "1"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"TwoPointTaylor", "[", 
    RowBox[{"f", ",", "a", ",", "b", ",", "1"}], "]"}], "=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"coeffs", ",", "c1", ",", "d1", ",", "linf"}], "}"}], ",", 
     RowBox[{
      RowBox[{"linf", "=", 
       RowBox[{
        RowBox[{"c1", "+", 
         RowBox[{"d1", " ", "#"}]}], " ", "&"}]}], ";", 
      RowBox[{"coeffs", "=", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"f", "[", "a", "]"}], "\[Equal]", 
            RowBox[{"linf", "[", "a", "]"}]}], ",", 
           RowBox[{
            RowBox[{"f", "[", "b", "]"}], "\[Equal]", 
            RowBox[{"linf", "[", "b", "]"}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"c1", ",", "d1"}], "}"}]}], "]"}]}], ";", 
      RowBox[{"linf", "/.", 
       RowBox[{"coeffs", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}]}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TwoPointTaylor", "[", 
   RowBox[{"f_", ",", "a_", ",", "b_", ",", "n_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"TwoPointTaylor", "[", 
    RowBox[{"f", ",", "a", ",", "b", ",", "n"}], "]"}], "=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"coeff", ",", 
       RowBox[{"dfnm1", "=", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], ",", "newterm", ",", 
       "d2pTnm1", ",", "c", ",", "d"}], "}"}], ",", 
     RowBox[{
      RowBox[{"newterm", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"c", "+", 
           RowBox[{"d", " ", "#"}]}], ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"#", "-", "a"}], ")"}], 
            RowBox[{"(", 
             RowBox[{"#", "-", "b"}], ")"}]}], ")"}], 
          RowBox[{"n", "-", "1"}]]}], "&"}]}], ";", 
      RowBox[{"d2pTnm1", "=", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"newterm", "[", "x", "]"}], "+", 
          RowBox[{
           RowBox[{"TwoPointTaylor", "[", 
            RowBox[{"f", ",", "a", ",", "b", ",", 
             RowBox[{"n", "-", "1"}]}], "]"}], "[", "x", "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
      RowBox[{"coeff", "=", 
       RowBox[{"Solve", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"dfnm1", "\[Equal]", "d2pTnm1"}], ")"}], "/.", 
            RowBox[{"x", "\[Rule]", "a"}]}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"dfnm1", "\[Equal]", "d2pTnm1"}], ")"}], "/.", 
            RowBox[{"x", "\[Rule]", "b"}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"c", ",", "d"}], "}"}]}], "]"}]}], ";", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"newterm", "[", "#", "]"}], "+", 
          RowBox[{
           RowBox[{"TwoPointTaylor", "[", 
            RowBox[{"f", ",", "a", ",", "b", ",", 
             RowBox[{"n", "-", "1"}]}], "]"}], "[", "#", "]"}]}], ")"}], "/.", 
        RowBox[{"coeff", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "&"}]}]}], "]"}]}]}]}], "Input",
